    },
    "license": "GPL-3.0-only",
    "dependencies": {
        "@chakra-ui/react": "^2.2.4",
        "@emotion/react": "^11.10.0",
        "@emotion/styled": "^11.10.0",
        "@railmapgen/rmg-components": "^3.0.2",
        "@railmapgen/rmg-palette-resources": "^0.3.29",
        "@railmapgen/rmg-runtime": "^0.1.4",
        "@railmapgen/rmg-templates-resources": "^0.1.15",
        "@railmapgen/rmg-translate": "^0.3.0",
        "@reduxjs/toolkit": "^1.8.5",
        "ag-grid-community": "^28.1.1",
        "ag-grid-react": "^28.1.1",
        "electron-is-dev": "^2.0.0",
        "framer-motion": "^7.2.1",
        "i18next": "^21.9.1",
        "jszip": "^3.10.1",
        "nanoid": "^3.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^11.18.5",
        "react-icons": "^4.4.0",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.56",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-mock-store": "^1.0.3",
        "electron": "^17.2.0",
        "electron-builder": "^22.14.13",
        "eslint-plugin-prettier": "^4.2.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.5.1",
        "prettier": "^2.7.1",
        "react-scripts": "^5.0.1",
        "redux-mock-store": "^1.5.4",
        "typescript": "^4.8.2",
        "wait-on": "^5.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false",
        "lint": "eslint -c ./.eslintrc.dev.json ./src",
        "lint:fix": "eslint ./src --fix",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
        "electron-package": "npx electron-builder build -c.extraMetadata.main=build/electron.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "version": "5.5.12",
    "main": "public/electron.js",
    "build": {
        "publish": [
            {
                "provider": "github",
                "vPrefixedTagName": false
            }
        ],
        "snap": {
            "publish": "github"
        }
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!(@railmapgen/.*))"
        ],
        "moduleNameMapper": {
            "\\.(css|less)$": "identity-obj-proxy"
        }
    }
}
